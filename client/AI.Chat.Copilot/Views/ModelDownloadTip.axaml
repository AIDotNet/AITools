<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="AI.Chat.Copilot.ModelDownloadTip"
        Title="ModelDownloadTip"
		xmlns:mdxaml="https://github.com/whistyun/Markdown.Avalonia.Tight"
		xmlns:vm="using:AI.Chat.Copilot.ViewModels"
		x:DataType="vm:ModelDownloadTipViewModel"
		 WindowStartupLocation="CenterScreen"
		 CanResize="False" Width="600" Height="300">
  <mdxaml:MarkdownScrollViewer  SelectionEnabled="True" Margin="0,5,0,0" Markdown="{Binding MdText}">
  </mdxaml:MarkdownScrollViewer>
</Window>
